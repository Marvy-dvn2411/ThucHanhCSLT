#include <stdio.h>
#include <math.h>
int main(){
	int n,i,a,b;
	printf("nhap kich thuoc pixel: ");
	scanf("%d",&n);
	for(int i=1; i<=sqrt(n); i++){
		if(n%i==0){
			int A=n/i;
			int B=i;
			if(abs(A-B)<abs(a-b)){
				a=A;
				b=B; 
			} 
		} 
	} 
	printf("chieu dai va chieu rong cua TV la: %d %d", a, b);
	return 0; 
} 
